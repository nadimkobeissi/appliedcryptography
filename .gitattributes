# PDFs
*.pdf filter=lfs diff=lfs merge=lfs -text

# Images
*.jpeg filter=lfs diff=lfs merge=lfs -text
*.jpg filter=lfs diff=lfs merge=lfs -text
*.webp filter=lfs diff=lfs merge=lfs -text
*.png filter=lfs diff=lfs merge=lfs -text
*.gif filter=lfs diff=lfs merge=lfs -text
*.svg filter=lfs diff=lfs merge=lfs -text
*.ico filter=lfs diff=lfs merge=lfs -text
*.avif filter=lfs diff=lfs merge=lfs -text

# Fonts
*.ttf filter=lfs diff=lfs merge=lfs -text linguist-vendored
*.otf filter=lfs diff=lfs merge=lfs -text linguist-vendored
*.woff filter=lfs diff=lfs merge=lfs -text linguist-vendored
*.woff2 filter=lfs diff=lfs merge=lfs -text linguist-vendored

# Vendored code
website/res/js/vendor/** filter=lfs diff=lfs merge=lfs -text linguist-vendored
